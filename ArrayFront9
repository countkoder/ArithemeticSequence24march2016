public boolean arrayFront9(int[] nums) {

//if(nums.length==0) return false;

if(nums.length==1 && nums[0]==9) {return true;}
if(nums.length==2) {for(int i=0; i<2; i++) if(nums[i]==9) return true;} 
if(nums.length==3) {for(int i=0; i<3; i++) if(nums[i]==9) return true;}
if(nums.length>3) {for(int i=0; i<4; i++) if(nums[i]==9) return true;}
  
return false; 
}
